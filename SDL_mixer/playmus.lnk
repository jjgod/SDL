#=============================================================================
#      This is a linker file to build SDLMIXER test app for OS/2
#
#
#=============================================================================

system os2v2

file playmus

OPTION MAP=playmus
OPTION DESCRIPTION 'Simple DirectMedia Layer Mixer v1.2.5 - Test Application'
OPTION ELIMINATE
OPTION MANYAUTODATA
OPTION OSNAME='OS/2 and eComStation'
OPTION SHOWDEAD
LIBPATH %os2tk%\lib
LIBPATH %sdlhome%\src
LIBPATH .
lib sdl
lib sdlmixer
